[program:spark-worker]
command=spark-class org.apache.spark.deploy.worker.Worker spark://{{ groups['spark-master-node'][0] }}:{{ spark_master_port }} --work-dir {{ spark_worker_dir }}
autostart=true
autorestart=true
stopsignal=KILL
redirect_stderr=true
stderr_logfile={{supervisord_log_dir }}/spark-master
stderr_logfile_maxbytes={{ spark_stderr_logfile_maxbytes }}
